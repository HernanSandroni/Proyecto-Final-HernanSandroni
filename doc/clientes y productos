   Parte 2: creación de productos

           Product zapatilla = new Product();
           zapatilla.setCode("abcdefg");
           zapatilla.setDescription("edicion limitada");
           zapatilla.setPrice(20000);
           zapatilla.setStock(100);
           this.productoRepository.save(zapatilla);

           Product auris = new Product();
           auris.setCode("dbz-generation");
           auris.setDescription("larga duracion");
           auris.setPrice(7500);
           auris.setStock(300);
           this.productoRepository.save(auris);

           Product camiseta = new Product();
           camiseta.setCode("x-e-n-e-i-z-e");
           camiseta.setDescription("original nike");
           camiseta.setPrice(32000);
           camiseta.setStock(200);
           this.productoRepository.save(camiseta);